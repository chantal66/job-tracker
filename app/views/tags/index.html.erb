
<div class="container">
  <div class="page-header">
    <h3>All Tags</h3>
    <div class="btn-toolbar pull-right">
      <%= link_to 'Create a new a Tag', new_tag_path, class: 'btn btn-default' %>
    </div>
  </div>

<% @tags.each do |tag| %>
  <%= tag.title %>
    <%#= link_to 'Edit', edit_tag_path(tag), class: 'btn btn-warning' %>
    <%= link_to 'Delete', tag_path(tag), method: 'delete', class: 'btn btn-danger' %>
<% end %>
</div>




